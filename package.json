    "eslint": "^5.15.2",
    "eslint-plugin-react": "^7.12.4",
    "gatsby": "^2.2.0-alpha.6",
    "gh-pages": "^2.0.1",
    "inquirer": "^6.2.2",
    "prettier": "^1.16.4",
